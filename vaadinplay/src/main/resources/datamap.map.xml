<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/10/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/10/modelMap http://cayenne.apache.org/schema/10/modelMap.xsd"
	 project-version="10">
	<property name="defaultPackage" value="cayenne"/>
	<db-entity name="JOIN_PARTY_SONG">
		<db-attribute name="CHILD_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="PARENT_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="PARTY">
		<db-attribute name="CREATED" type="TIMESTAMP"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="LAST_MODIFIED" type="TIMESTAMP"/>
		<db-attribute name="NAME" type="VARCHAR" length="64"/>
	</db-entity>
	<db-entity name="SONG">
		<db-attribute name="ARTIST_NAME" type="VARCHAR" length="64"/>
		<db-attribute name="CREATED" type="TIMESTAMP"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="LAST_MODIFIED" type="TIMESTAMP"/>
		<db-attribute name="NAME" type="VARCHAR" length="64"/>
	</db-entity>
	<db-entity name="USER">
		<db-attribute name="CREATED" type="TIMESTAMP"/>
		<db-attribute name="EMAIL_ADDRESS" type="VARCHAR" length="128"/>
		<db-attribute name="FIRST_NAME" type="VARCHAR" length="48"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="ITEM_FLAGS" type="VARCHAR" length="32"/>
		<db-attribute name="LAST_MODIFIED" type="TIMESTAMP"/>
		<db-attribute name="LAST_NAME" type="VARCHAR" length="48"/>
		<db-attribute name="PARTY_ID" type="INTEGER"/>
		<db-attribute name="PASSWORD" type="VARCHAR" length="256"/>
	</db-entity>
	<obj-entity name="Party" className="cayenne.Party" dbEntityName="PARTY">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
		<obj-attribute name="created" type="java.time.LocalDateTime" db-attribute-path="CREATED"/>
		<obj-attribute name="lastModified" type="java.time.LocalDateTime" db-attribute-path="LAST_MODIFIED"/>
	</obj-entity>
	<obj-entity name="Song" className="cayenne.Song" dbEntityName="SONG">
		<obj-attribute name="created" type="java.time.LocalDateTime" db-attribute-path="CREATED"/>
		<obj-attribute name="lastModified" type="java.time.LocalDateTime" db-attribute-path="LAST_MODIFIED"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
		<obj-attribute name="artistName" type="java.lang.String" db-attribute-path="ARTIST_NAME"/>
	</obj-entity>
	<obj-entity name="User" className="cayenne.User" dbEntityName="USER">
		<obj-attribute name="created" type="java.time.LocalDateTime" db-attribute-path="CREATED"/>
		<obj-attribute name="emailAddress" type="java.lang.String" db-attribute-path="EMAIL_ADDRESS"/>
		<obj-attribute name="firstName" type="java.lang.String" db-attribute-path="FIRST_NAME"/>
		<obj-attribute name="itemFlags" type="java.lang.String" db-attribute-path="ITEM_FLAGS"/>
		<obj-attribute name="lastModified" type="java.time.LocalDateTime" db-attribute-path="LAST_MODIFIED"/>
		<obj-attribute name="lastName" type="java.lang.String" db-attribute-path="LAST_NAME"/>
		<obj-attribute name="password" type="java.lang.String" db-attribute-path="PASSWORD"/>
		<obj-attribute name="partyID" type="int" db-attribute-path="PARTY_ID"/>
	</obj-entity>
	<db-relationship name="party" source="JOIN_PARTY_SONG" target="PARTY">
		<db-attribute-pair source="PARENT_ID" target="ID"/>
	</db-relationship>
	<db-relationship name="song" source="JOIN_PARTY_SONG" target="SONG">
		<db-attribute-pair source="CHILD_ID" target="ID"/>
	</db-relationship>
	<db-relationship name="party" source="PARTY" target="JOIN_PARTY_SONG" toDependentPK="true" toMany="true">
		<db-attribute-pair source="ID" target="PARENT_ID"/>
	</db-relationship>
	<db-relationship name="song" source="SONG" target="JOIN_PARTY_SONG" toDependentPK="true" toMany="true">
		<db-attribute-pair source="ID" target="CHILD_ID"/>
	</db-relationship>
	<obj-relationship name="songs" source="Party" target="Song" deleteRule="Nullify" db-relationship-path="party.song"/>
	<obj-relationship name="parties" source="Song" target="Party" deleteRule="Nullify" db-relationship-path="song.party"/>
	<config xmlns="http://cayenne.apache.org/schema/10/dbimport">
		<forceDataMapCatalog>false</forceDataMapCatalog>
		<forceDataMapSchema>false</forceDataMapSchema>
		<namingStrategy>org.apache.cayenne.dbsync.naming.DefaultObjectNameGenerator</namingStrategy>
		<skipPrimaryKeyLoading>false</skipPrimaryKeyLoading>
		<skipRelationshipsLoading>false</skipRelationshipsLoading>
		<useJava7Types>false</useJava7Types>
		<usePrimitives>true</usePrimitives>
	</config>
</data-map>
